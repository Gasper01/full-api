{
  "builds": [
    { "src": "src/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/src/index.js" }
  ],
  "build": {
    "src": "src",
    "use": "@vercel/build-utils/lambda",
    "config": {
      "command": "npm run build",
      "outputDirectory": "dist"
    }
  }
}
